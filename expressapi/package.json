	"author": "olivierhuvelle",
	"license": "ISC",
	"bugs": {
		"url": "https://github.com/olivierHuvelle/ecommerce/issues"
	},
	"homepage": "https://github.com/olivierHuvelle/ecommerce#readme",
	"dependencies": {
		"awilix": "^10.0.1",
		"cors": "^2.8.5",
		"dotenv": "^16.4.1",
		"express": "^4.18.3",
		"express-rate-limit": "^7.2.0",
		"helmet": "^7.1.0",
		"http-errors": "^2.0.0",
		"i18next": "^23.10.1",
		"i18next-fs-backend": "^2.3.1",
		"i18next-http-middleware": "^3.5.0",
		"swagger-ui-express": "^5.0.0",
