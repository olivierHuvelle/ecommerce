{
	"name": "expressecommerce",
	"version": "1.0.0",
	"description": "simple api for an ecommerce",
	"main": "index.js",
	"scripts": {
		"dev": "NODE_ENV=DEV nodemon --exec babel-node ./src/bin/www.js",
