		"@babel/preset-env": "^7.22.5",
		"@babel/register": "^7.22.5",
		"babel-plugin-module-resolver": "^5.0.0",
		"chai": "^4.3.7",
		"chai-http": "^4.4.0",
		"eslint": "^8.56.0",
		"lint-staged": "^15.2.2",
		"mocha": "^10.2.0",
		"nodemon": "^3.0.3",
		"prettier": "3.2.4"
